body, #content, #container {
	width: 100%;
	margin: 0;
	float: none;
	background: #fff url(none);
}

#topnav, #navbar, #nav, #sidebar, .ad, .noprint {
	display: none; 
}

body {
	font: 1em Georgia, "Times New Roman", Times, serif;
	color: #000; 
}

a:link, a:visited {
	color: #00c;
	font-weight: bold;
	text-decoration: underline; }
#content a:link:after, #content a:visited:after {
	content: " (" attr(href) ") ";
}
